{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "singularName": "blog-post",
    "pluralName": "blog-posts",
    "displayName": "Blog Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "required": true
    },
    "Content": {
      "type": "blocks",
      "required": true
    },
    "blog_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-category.blog-category"
    },
    "Tags": {
      "type": "enumeration",
      "enum": [
        "tips",
        "hygiene",
        "checkup",
        "prevention"
      ]
    }
  }
}
